{
    "name": "kitapyurdu/bkm-express-php",
    "type": "library",
    "description": "BKM Express PHP Client Library",
    "authors": [
        {
            "name": "Bex Developer Team"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-0": {
            "Bex": "src/main/"
        },
        "psr-4": {
            "Bex\\": "src/main/Bex"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "BexTest": "src/tests/"
        },
        "psr-4": {
            "BexTest\\": "src/tests/BexTest"
        }
    }
}
